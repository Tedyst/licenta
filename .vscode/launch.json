{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}"
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "serve",
                "-d"
            ],
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Analyze this Git repo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "extract",
                "git",
                "."
            ],
            "program": "${workspaceFolder}"
        },
        {
            "name": "Runlocal",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "servelocal",
                "-d"
            ]
        },
        {
            "name": "Worker",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "worker",
                "--worker-token",
                "asdasd",
                "-d"
            ]
        },
        {
            "name": "Docker extract",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "extract",
                "docker",
                "test",
                "--local",
                "-d"
            ]
        },
        {
            "name": "CI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "ci",
                "--project",
                "1",
                "--worker-token",
                "asdasd",
                "-d"
            ]
        },
        {
            "name": "Run Scan Mysql ID 1",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "tasks",
                "scanmysql",
                "1",
                "-d"
            ]
        },
        {
            "name": "Run Scan Redis",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "scan",
                "redis",
                "localhost",
                "6379",
                ""
            ]
        },
        {
            "name": "Run Scan MongoDB",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "scan",
                "mongodb",
                "mongodb://root:password@localhost:27017",
            ]
        },
        {
            "name": "Run Scheduler",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "scheduler",
                "-d"
            ]
        },
        {
            "command": "npx vavite-loader vite dev --port 3000",
            "name": "Launch SvelteKit server",
            "request": "launch",
            "type": "node-terminal",
            "cwd": "${workspaceFolder}/frontend"
        }
    ]
}