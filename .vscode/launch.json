    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "serve",
                "-d"
            ],
            "program": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env"
        },
        {
            "name": "Analyze this Git repo",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "extract",
                "git",
                "."
            ],
            "program": "${workspaceFolder}"
        },
        {
            "name": "Runlocal",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "servelocal"
            ]
        },
        {
            "name": "Worker",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "worker",
                "--worker-token",
                "asdasd",
                "-d"
            ]
        },
        {
            "name": "Docker extract",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "extract",
                "docker",
                "test",
                "--local",
                "-d"
            ]
        },
        {
            "name": "CI",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": ".",
            "args": [
                "ci",
                "--project",
                "1",
                "--worker-token",
                "asdasd",
                "-d"
            ]
        }
    ]
}